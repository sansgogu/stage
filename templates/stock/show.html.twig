<!DOCTYPE html>

<title>Stock</title>

{% block body %}
    <h1>Stock</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ stock.id }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ stock.adresse }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ stock.nom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ stock.telephone }}</td>
            </tr>
        </tbody>
    </table>

 
    <a href="{{ path('app_stock_edit', {'id': stock.id}) }}">edit</a>

    {{ include('stock/_delete_form.html.twig') }}
{% endblock %}
